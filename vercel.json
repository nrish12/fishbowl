{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.mystle.app"
        }
      ],
      "destination": "https://mystle.app/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/s/:code",
      "destination": "/index.html"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
